{
    "name": "hazardteam/lottery",
    "license": "MIT",
    "type": "project",
    "require": {
        "dapigguy/libpiggyeconomy": "dev-master",
        "latampmdevs/formapi": "dev-master",
        "muqsit/invmenu": "^4.6",
        "paroxity/commando": "^3.2",
        "pocketmine/pocketmine-mp": "^5.3"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-strict-rules": "^1.5"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/LatamPMDevs/FormAPI.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/DaPigGuy/libPiggyEconomy.git"
        }
    ],
    "autoload": {
        "classmap": [
            "src"
        ]
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
